{
  "name": "@vertigoamm/vertigo-sdk",
  "version": "0.1.0",
  "description": "SDK for interacting with the Vertigo AMM protocol",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "solana",
    "anchor",
    "vertigo",
    "amm",
    "sdk"
  ],
  "author": "Vertigo AMM",
  "license": "ISC",
  "scripts": {
    "lint:fix": "prettier */*.js \"*/**/*{.js,.ts}\" -w",
    "lint": "prettier */*.js \"*/**/*{.js,.ts}\" --check",
    "build": "tsc && bun build src/index.ts --outdir ./dist --target node",
    "build:types": "tsc --emitDeclarationOnly",
    "prebuild": "rm -rf dist",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@coral-xyz/anchor-cli": "^0.30.1",
    "@solana/spl-token": "^0.4.12",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "chai": "^4.3.4",
    "mocha": "^9.0.3",
    "prettier": "^2.6.2",
    "ts-mocha": "^10.0.0",
    "typescript": "~4.9.5"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
