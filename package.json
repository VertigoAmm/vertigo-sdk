{
  "name": "@vertigo-amm/vertigo-sdk",
  "version": "0.1.6",
  "description": "SDK for interacting with the Vertigo AMM protocol",
  "bin": {
    "vertigo": "./dist/cli/index.js"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "solana",
    "anchor",
    "vertigo",
    "amm",
    "sdk"
  ],
  "author": "Vertigo AMM",
  "license": "ISC",
  "scripts": {
    "lint:fix": "prettier */*.js \"*/**/*{.js,.ts}\" -w",
    "lint": "prettier */*.js \"*/**/*{.js,.ts}\" --check",
    "build": "tsc && npm run copy-files || echo 'tsc failed but continuing...' && npm run copy-files",
    "prebuild": "rm -rf dist",
    "test": "ts-mocha -p ./tsconfig.json tests/**/*.ts",
    "copy-files": "echo 'Starting copy...' && mkdir -p dist/target && cp -r target/* dist/target/ && echo 'Files copied!'",
    "cli:dev": "ts-node src/cli/index.ts"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@coral-xyz/anchor-cli": "^0.30.1",
    "@solana/spl-token": "^0.4.12",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "inquirer": "^12.5.2",
    "ts-node": "^10.9.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^22.14.1",
    "@types/yargs": "^17.0.33",
    "chai": "^4.3.4",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.4.7",
    "mocha": "^9.0.3",
    "prettier": "^2.6.2",
    "ts-mocha": "^10.0.0",
    "typescript": "~4.9.5",
    "yargs": "^17.7.2"
  }
}
